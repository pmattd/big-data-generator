{
  "file-write-interval-in-seconds": 2,
  "path": "/dir/files",
  "max-lines": 100,
  "line-write-interval-in-seconds": 1,
  "value-separator": ",",
  "max-files": 1,
  "base-filename": "data_",
  "fields": [
    {
      "type": "identity",
      "min": 1,
      "max": 100
    },
    {
      "type": "random-value",
      "min": 1,
      "max": 100
    },
    {
      "type": "enumeration",
      "values": [
        "peter",
        "paul",
        "mary"
      ]
    }
  ]
}